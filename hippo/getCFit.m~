function layer = getCFit(cont)

%pos = cont(1,1);
state = 1;
for i = 1:Inf
    layers{i}.height = cont(1,state);
    numElems = cont(2,state);
    j = 0;
    layers{i}.vals = [];
    while j < numElems
        j = j + 1;
        try
            layers{i}.vals(length(layers{i}.vals)+1,:) = cont(:,state+j);
        catch
            layers{i}.vals
        end
    end
    state = state + j + 1;
    if state > size(cont,2)
        break
    end
end

state = 1;
for i = 1:length(layers)
    layer{state}.height = layers{i}.height;
    layer{state}.vals = layers{i}.vals;

    while layers{state}.height == layers{i}.height
        i = i+1;
        layer{state}.vals = [layer{state}.vals; layers{i}.vals];
        state = state + 1;
        [i state]
    end 
end