function hilbertH5(input,output,numChans)

h5filecreate(output);
[~,~,nSamples] = LoadBinary(input,1,[],[],[],[],[1 2]);
h5datacreate(output,'/h','type','single','size',[numChans nSamples]);
for i = 1:numChans
a = LoadBinary('ec014.277/ec014.277.eeg',i,[],[],[],'int16',[]);
h = hilbert(single(a));
h5varput(output,'/h',[i-1 0],[1 size(h,2)],h);
i
end